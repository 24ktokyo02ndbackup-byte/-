local uiVisSec = Visuals:section({ name = "UI", side = "right" })
uiVisSec:toggle({
	name = "Gooner Anime girl background",
	flag = "anime_girl_bg",
	default = getgenv().Zero.UI.AnimeGirlBackground,
	callback = function(v)
    getgenv().Zero.UI.AnimeGirlBackground = v
    for _, child in getgenv().library.gui:GetChildren() do
        if child:IsA("ImageLabel") then
            if v then
				getgenv().Zero.UI.AnimeGirlBackground = true
                child.Image = "rbxassetid://15309876546" --6311243693 also good
                child.Visible = true
            else
				getgenv().Zero.UI.AnimeGirlBackground = false
                child.Image = ""
                child.Visible = false
            end
            break
        end
    end
end
})
-- ========== CONFIGS TAB ==========

local config_section = Configs:section({ name = "Configuration System", side = "left" })

local dir = "zeroconfigs/"

-- Create directory if it doesn't exist
pcall(function()
    if not isfolder(dir) then
        makefolder(dir)
    end
end)

-- Function to get all config files
local function getConfigs()
    local configs = {}
    pcall(function()
        local files = listfiles(dir)
        for _, file in ipairs(files) do
            local name = string.match(file, dir .. "(.+)%.cfg")
            if name then
                table.insert(configs, name)
            end
        end
    end)
    return configs
end

-- Store references to avoid obfuscation issues
local config_name_list_flag = "config_name_list"
local config_name_text_flag = "config_name_text_box"

-- Populate dropdown
getgenv().library.config_holder = config_section:dropdown({
    name = "Configs",
    items = getConfigs(),
    flag = config_name_list_flag,
    multi = false
})

-- Textbox for new config name
config_section:textbox({
    flag = config_name_text_flag
})

-- Helper functions to avoid inline concatenation
local function getConfigPath(name)
    return dir .. tostring(name) .. ".cfg"
end

local function getConfigName(flagName)
    return getgenv().flags[flagName]
end

-- Create button
config_section:button({
    name = "Create",
    callback = function()
        pcall(function()
            local name = getConfigName(config_name_text_flag)
            if name and name ~= "" then
                local path = getConfigPath(name)
                local configData = getgenv().library:get_config()
                writefile(path, configData)
                getgenv().library:notification({ text = "Config created: " .. tostring(name) }, 5)
                
                -- Refresh dropdown
                getgenv().library.config_holder:update_list(getConfigs())
            end
        end)
    end
})

-- Delete button
config_section:button({
    name = "Delete",
    callback = function()
        pcall(function()
            local name = getConfigName(config_name_list_flag)
            if name then
                getgenv().library:panel({
                    name = "Are you sure you want to delete " .. tostring(name) .. "?",
                    options = { "Yes", "No" },
                    callback = function(option)
                        if option == "Yes" then
                            pcall(function()
                                local path = getConfigPath(name)
                                delfile(path)
                                getgenv().library:notification({ text = "Config deleted: " .. tostring(name)}, 5)
                                
                                -- Refresh dropdown
                                getgenv().library.config_holder:update_list(getConfigs())
                            end)
                        end
                    end
                })
            end
        end)
    end
})

-- Load button
config_section:button({
    name = "Load",
    callback = function()
        pcall(function()
            local name = getConfigName(config_name_list_flag)
            if name then
                local path = getConfigPath(name)
                local configData = readfile(path)
                getgenv().library:load_config(configData)
                getgenv().library:notification({ text = "Config loaded: " .. tostring(name)}, 5)
            end
        end)
    end
})

-- Save button
config_section:button({
    name = "Save",
    callback = function()
        pcall(function()
            local name = getConfigName(config_name_list_flag)
            if name then
                local path = getConfigPath(name)
                local configData = getgenv().library:get_config()
                writefile(path, configData)
                getgenv().library:notification({ text = "Config saved: " .. tostring(name) }, 5)
            end
        end)
    end
})

-- Unload button
config_section:button({
    name = "Unload Config",
    callback = function()
        pcall(function()
            if getgenv().library.old_config then
                getgenv().library:load_config(getgenv().library.old_config)
                getgenv().library:notification({ text = "Config unloaded" }, 5)
            end
        end)
    end
})

config_section:button({
    name = "Unload Menu",
    callback = function()
        pcall(function()
            getgenv().library:unload()
        end)
    end
})


task.wait()
pcall(function()
    getgenv().library.old_config = getgenv().library:get_config()
end)



