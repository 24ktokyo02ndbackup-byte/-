getgenv().library = loadstring(game:HttpGet("https://pastebin.com/raw/U2YcuUDn"))()
getgenv().flags = library.flags	

local function GetSize()
    if getgenv().IsMobile then 
        return UDim2.fromOffset(550, 450)
    else 
        return UDim2.fromOffset(550, 650)
     end
 end

-- Window
local window = getgenv().library:window({
    name = "Zero",
    size = GetSize(),
	background_image = ""
})

-- Tabs
local Main = window:tab({ name = "Main" })
local Visuals = window:tab({ name = "Visuals" })
local Configs = window:tab({ name = "Configs" })

